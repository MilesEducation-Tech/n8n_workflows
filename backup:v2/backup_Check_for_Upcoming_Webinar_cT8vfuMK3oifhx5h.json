  "name": "Check for Upcoming Webinar",
  "active": false,
  "nodes": [
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Metabase variable: {{days_ahead}} (number of days after today)\n-- e.g., days_ahead = 3 â†’ fetches events for next 3 days (excluding today)\n\nWITH boundaries AS (\n  SELECT\n    -- Start of tomorrow in IST, converted to UTC\n    (DATE_TRUNC('day', (NOW() + INTERVAL '{{ $json.start_interval }} day') AT TIME ZONE 'Asia/Kolkata') AT TIME ZONE 'Asia/Kolkata' AT TIME ZONE 'UTC') AS start_utc,\n    \n    -- End date = start + {{days_ahead}} days, also in UTC\n    (DATE_TRUNC('day', (NOW() + ({{ $json.end_interval }} + 1 || ' day')::interval) AT TIME ZONE 'Asia/Kolkata') AT TIME ZONE 'Asia/Kolkata' AT TIME ZONE 'UTC') AS end_utc\n)\nSELECT \n  w.*,\n  (w.date_time AT TIME ZONE 'UTC' AT TIME ZONE 'Asia/Kolkata')::date AS ist_date\nFROM public.webinar w\nJOIN boundaries b ON\n  w.date_time >= b.start_utc AND w.date_time < b.end_utc\nwhere w.is_meet_and_greet is false\nORDER BY w.date_time;",
        "options": {}
      },
